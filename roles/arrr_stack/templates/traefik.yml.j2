log:
  #level: DEBUG
  level: INFO
api:
  dashboard: true
  insecure: true
  debug: false
entryPoints:
  http:
    address: ":80"
  https:
    address: ":443"
ping:
  entryPoint: "http"
serversTransport:
  insecureSkipVerify: true
providers:
  docker:
    endpoint: "unix:///var/run/docker.sock"
    exposedByDefault: false
  #file:
  #  filename: /config.yml
  #  #directory: /configs
  #  watch: true
#metrics:
#  prometheus: {}
certificatesResolvers:
  cloudflare:
    acme:
      email: {{ cert_resolver_email }}
      storage: acme.json
      dnsChallenge:
        provider: cloudflare
        resolvers:
          - "1.1.1.1:53"
          - "1.0.0.1:53"
