log:
  #level: DEBUG
  level: INFO
api:
  dashboard: true
  insecure: true
  debug: false
entryPoints:
  http:
    address: ":80"
  https:
    address: ":443"
serversTransport:
  insecureSkipVerify: true
providers:
  docker:
    endpoint: "unix:///var/run/docker.sock"
    exposedByDefault: false
  file:
    #filename: /config.yml
    directory: /configs
    watch: true
